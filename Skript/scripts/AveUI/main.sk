import:
	java.lang.management.ManagementFactory
	javax.management.ObjectName

 
function getCpuLoad() :: number:
    set {_objectName} to ObjectName.getInstance("java.lang:type=OperatingSystem")
    set {_factory} to ManagementFactory.getPlatformMBeanServer()
    set {_attributeList} to {_factory}.getAttributes({_objectName}, ["ProcessCpuLoad" as String])
    return {_attributeList}.get(0).getValue()
 
function getFreeMemory() :: number:
	set {_fm} to difference between max memory/(1024*1024) and used memory of server/(1024*1024)
	return {_fm}

#------------------------------------------------------
on script load:
	set {-gcinfo} to "&a–ò–¥–µ–∞–ª—å–Ω–æ"
	set {-apprx} to 0

command /gc:
	aliases: aveui:gc
	trigger:
		set {_v} to executor
		if executor is not a player:
			set {_v} to "&aConsole"
		else:
			player doesn't have permission "avpack.moder"
			stop
		send "&f"
		send "&6 ‚ñé &6‚àæ &x&d&b&d&5&c&3–í—Ä–µ–º—è —Ä–∞–±–æ—Ç—ã —Å–µ—Ä–≤–µ—Ä–∞: &a%uptime of server%"
		send "&6 ‚ñé &6‚àæ &x&d&b&d&5&c&3–¢–ü–° –°–µ—Ä–≤–µ—Ä–∞: %{-gcinfo}%"
		send "&6 ‚ñé &f"
		send "&6 ‚ñé &6‚ñ† &x&d&b&d&5&c&3–Ø–¥—Ä–∞ –¶–ü: &a%amount of processor cores% —è–¥–µ—Ä" 
		send "&6 ‚ñé &6‚Ü≥ &x&d&b&d&5&c&3–ò—Å–ø–æ–ª—å–∑–æ–≤–∞–Ω–∏–µ –¶–ü: &a%getCpuLoad()*100%ÔºÖ"
		send "&6 ‚ñé &f"
		send "&6 ‚ñé &6‚à´ &x&d&b&d&5&c&3–°–≤–æ–±–æ–¥–Ω–∞—è –ø–∞–º—è—Ç—å: &a%getFreeMemory()%MB" 
		send "&6 ‚ñé &6‚à´ &x&d&b&d&5&c&3–ò—Å–ø–æ–ª—å–∑–æ–≤–∞–Ω–Ω–∞—è –ø–∞–º—è—Ç—å: &a%used memory of server/(1024*1024)%MB"
		send "&f"
every tick:
	set {_cpuload} to getCpuLoad()*100
	if {_cpuload} > 31:
		if {cpuload} > 59:
			{-apprx} > -51
			remove 2 from {-apprx}
		else:
			{-apprx} > -51
			remove 2 from {-apprx}
	else:
		{-apprx} < 66
		add 1 to {-apprx} 


	
every 1.6 seconds:
	if {-apprx} > -1:
		if {-apprx} < 34:
			set {-gcinfo} to "&6–°—Ç–∞–±–∏–ª—å–Ω–æ"
			set {-gcpro} to "&6%random number between 18.98 and 19.3% ‚Üì"
		else:
			set {-gcinfo} to "&a–ò–¥–µ–∞–ª—å–Ω–æ"
			set {-gcpro} to "&a%random number between 19.98 and 20% ‚Üë"
	else:
		if {-apprx} > -27:
			set {-gcinfo} to "&c–ü–ª–æ—Ö–æ"
			set {-gcpro} to "&c%{-currenttps}% ‚Üì"
		else:
			set {-gcinfo} to "&4–û—á–µ–Ω—å –ø–ª–æ—Ö–æ"
			set {-gcpro} to "&4%{-currenttps}% ‚Üì"
			
command /chatclear:
	aliases: aveui:clearchat
	trigger:
		set {_v} to executor
		if executor is not a player:
			set {_v} to "&aConsole"
		else:
