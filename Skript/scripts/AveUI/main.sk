			set {_v} to "&aConsole"
		else:
			player doesn't have permission "avpack.moder"
			stop
		arg-1 is set
		if arg-2 is set:
			set {_a} to arg-2
		else:
			set {_a} to "&7–ù–µ –£–∫–∞–∑–∞–Ω–∞"
		set {mut.reason.%player%} to {_a}
		set {mut.blocked.%player%} to arg-1
