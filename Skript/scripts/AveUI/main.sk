# Anka Chests & Other Security
#----------------------------


on join:
	set {-chestsbroken::%uuid of player%} to 0
	set {-chestviolates::%uuid of player%} to 0
	set {-ignitetimes::%uuid of player%} to 0
	set {-zabrokdchest::%uuid of player%} to false
	set {-lstjointime::%uuid of player%} to now

on break:
	if difference between now and {-lstjointime::%uuid of player%} > 35 minutes:
		set {-chestsbroken::%uuid of player%} to 0
		set {-chestviolates::%uuid of player%} to 0
		set {-ignitetimes::%uuid of player%} to 0
		set {-zabrokdchest::%uuid of player%} to false
		set {-lstjointime::%uuid of player%} to now
	past event-block is chest
	if time played of player is less than 5 hours:
		add 1 to {-chestsbroken::%uuid of player%}
		if time played of player is less than 38 minutes:
			{-chestsbroken::%uuid of player%} > 4
			if {-zabrokdchest::%uuid of player%} is false:
				loop all players in radius 37 of player:
					add 1 to {_f}
				{_f} < 3
				add 1 to {-chestviolates::%uuid of player%}
			set {-zabrokdchest::%uuid of player%} to true
	if {-chestviolates::%uuid of player%} > 1:
		execute console command "ban %player% –ó–∞–±–ª–æ–∫–∏—Ä–æ–≤–∞–Ω –≤ —Ü–µ–ª—è—Ö –±–µ–∑–æ–ø–∞—Å–Ω–æ—Å—Ç–∏ –∞–ª–≥–æ—Ä–∏—Ç–º–∞–º–∏"

on block ignite:
	if difference between now and {-lstjointime::%uuid of player%} > 35 minutes:
		set {-chestsbroken::%uuid of player%} to 0
		set {-chestviolates::%uuid of player%} to 0
		set {-ignitetimes::%uuid of player%} to 0
		set {-zabrokdchest::%uuid of player%} to false
		set {-lstjointime::%uuid of player%} to now
	add 1 to {-ignitetimes::%uuid of player%} 
	if {-ignitetimes::%uuid of player%} > 3:
		{-ignitetimes::%uuid of player%} < 100
		set {-ignitetimes::%uuid of player%} to 999
		add 1 to {-chestviolates::%uuid of player%}
	if {-chestviolates::%uuid of player%} > 1:
		execute console command "ban %player% –ó–∞–±–ª–æ–∫–∏—Ä–æ–≤–∞–Ω –≤ —Ü–µ–ª—è—Ö –±–µ–∑–æ–ø–∞—Å–Ω–æ—Å—Ç–∏ –∞–ª–≥–æ—Ä–∏—Ç–º–∞–º–∏"

on place:

	event-block is lava
	if difference between now and {-lstjointime::%uuid of player%} > 35 minutes:
		set {-chestsbroken::%uuid of player%} to 0
		set {-chestviolates::%uuid of player%} to 0
		set {-ignitetimes::%uuid of player%} to 0
		set {-zabrokdchest::%uuid of player%} to false
		set {-lstjointime::%uuid of player%} to now
	add 1 to {-ignitetimes::%uuid of player%} 
	time played of player is less than 5 hours
	if {-ignitetimes::%uuid of player%} >= 5:
		{-ignitetimes::%uuid of player%} < 455
		set {-ignitetimes::%uuid of player%} to 999
		add 2 to {-chestviolates::%uuid of player%}
	if {-ignitetimes::%uuid of player%} > 3:
		{-ignitetimes::%uuid of player%} < 5
		set {-ignitetimes::%uuid of player%} to 999
		add 1 to {-chestviolates::%uuid of player%}
	if {-chestviolates::%uuid of player%} > 1:
		execute console command "ban %player% –ó–∞–±–ª–æ–∫–∏—Ä–æ–≤–∞–Ω –≤ —Ü–µ–ª—è—Ö –±–µ–∑–æ–ø–∞—Å–Ω–æ—Å—Ç–∏ –∞–ª–≥–æ—Ä–∏—Ç–º–∞–º–∏"
